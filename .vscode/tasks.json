{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build"
			},
			"label": "release",
			"presentation": {
				"clear": true
			},
			"args": [
				"--release"
			]
		},
		{
			"type": "cargo",
			"command": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build"
			},
			"label": "clean",
			"presentation": {
				"clear": true
			}
		},
		{
			"type": "cargo",
			"command": "rustc",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "none"
			},
			"label": "build: emit llvm-ir",
			"presentation": {
				"clear": true
			},
			"args": [
				"--release",
				"--",
				"-C",
				"save-temps",
				"--emit=llvm-ir"
			],
			"hide": true
		},
		{
			"type": "shell",
			"label": "llvm-ir",
			"command": "xcopy",
			"args": [
				"${workspaceFolder}\\target\\release\\deps\\${workspaceFolderBasename}.ll",
				"${workspaceFolder}\\out",
				"/y"
			],
			"group": "build",
			"dependsOn": "build: emit llvm-ir",
			"dependsOrder": "sequence"
		},
		{
			"type": "cargo",
			"command": "rustc",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "none"
			},
			"label": "build: emit llvm-bc",
			"presentation": {
				"clear": true
			},
			"args": [
				"--release",
				"--",
				"-C",
				"save-temps",
				"--emit=llvm-bc"
			],
			"hide": true
		},
		{
			"label": "llvm-bc",
			"type": "shell",
			"command": "xcopy",
			"args": [
				"${workspaceFolder}\\target\\release\\deps\\${workspaceFolderBasename}.bc",
				"${workspaceFolder}\\out",
				"/y"
			],
			"group": "build",
			"dependsOn": "build: emit llvm-bc",
			"dependsOrder": "sequence"
		},
		{
			"type": "cargo",
			"command": "run",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			},
			"label": "run",
			"args": [
				"--release"
			]
		},
		{
			"label": "prepare temp files",
			"type": "shell",
			"command": "del ${workspaceFolder}\\target\\release\\deps\\${workspaceFolderBasename}.${workspaceFolderBasename}.*;xcopy ${workspaceFolder}\\target\\release\\deps\\${workspaceFolderBasename}.*.rcgu.o ${workspaceFolder}\\target\\release\\deps\\TEMP_FILE.${workspaceFolderBasename}.rcgu.o* /Y;del ${workspaceFolder}\\target\\release\\deps\\${workspaceFolderBasename}.*.rcgu.*",
			"problemMatcher": [],
			"hide": true
		},
		{
			"label": "clang: build analysis",
			"type": "shell",
			"command": "clang -m64 ${file} ${workspaceFolder}\\analysis\\print.ll -o ${workspaceFolder}\\analysis\\analysis.exe ${workspaceFolder}\\target\\release\\deps\\TEMP_FILE.mt_prototype.rcgu.o $env:RUSTLIB\\libstd-49e3d1aefc00cc02.rlib $env:RUSTLIB\\libpanic_abort-23a83f9cb9b3433e.rlib $env:RUSTLIB\\librustc_demangle-99c77609a4536a8b.rlib $env:RUSTLIB\\libstd_detect-af9ffb3c6d3f8ec7.rlib $env:RUSTLIB\\libhashbrown-a6bfe0548f994b2d.rlib $env:RUSTLIB\\librustc_std_workspace_alloc-61f07eb10bc24cc3.rlib $env:RUSTLIB\\libunwind-2fb3f9083307133a.rlib $env:RUSTLIB\\libcfg_if-6892c9ede6f0d6e6.rlib $env:RUSTLIB\\liblibc-788771c7bce73875.rlib $env:RUSTLIB\\liballoc-8a4c192e8601db8f.rlib $env:RUSTLIB\\librustc_std_workspace_core-4bf403d115b018c3.rlib $env:RUSTLIB\\libcore-48aa4c2213e4ac50.rlib $env:RUSTLIB\\libcompiler_builtins-34df001b737926cd.rlib -lws2_32 -luserenv -lntdll",
			"dependsOn": [
				"prepare temp files"
			],
			"dependsOrder": "sequence",
			"group": "build",
			"hide": true
		},
		{
			"label": "clang: analysis",
			"type": "shell",
			"command": "${workspaceFolder}\\analysis\\analysis.exe",
			"group": "build",
			"dependsOn": "clang: build analysis",
			"dependsOrder": "sequence"
		},
		{
			"label": "clang: build",
			"type": "shell",
			"command": "clang -m64 .\\out\\mt_prototype.ll -o .\\out\\mt_prototype-clang.exe .\\target\\release\\deps\\TEMP_FILE.mt_prototype.rcgu.o $env:RUSTLIB\\libstd-49e3d1aefc00cc02.rlib $env:RUSTLIB\\libpanic_abort-23a83f9cb9b3433e.rlib $env:RUSTLIB\\librustc_demangle-99c77609a4536a8b.rlib $env:RUSTLIB\\libstd_detect-af9ffb3c6d3f8ec7.rlib $env:RUSTLIB\\libhashbrown-a6bfe0548f994b2d.rlib $env:RUSTLIB\\librustc_std_workspace_alloc-61f07eb10bc24cc3.rlib $env:RUSTLIB\\libunwind-2fb3f9083307133a.rlib $env:RUSTLIB\\libcfg_if-6892c9ede6f0d6e6.rlib $env:RUSTLIB\\liblibc-788771c7bce73875.rlib $env:RUSTLIB\\liballoc-8a4c192e8601db8f.rlib $env:RUSTLIB\\librustc_std_workspace_core-4bf403d115b018c3.rlib $env:RUSTLIB\\libcore-48aa4c2213e4ac50.rlib $env:RUSTLIB\\libcompiler_builtins-34df001b737926cd.rlib -lws2_32 -luserenv -lntdll",
			"dependsOn": [
				"prepare temp files"
			],
			"dependsOrder": "sequence",
			"group": "build"
		},
		{
			"label": "create cfg-dot files",
			"type": "shell",
			"command": "opt -passes=dot-cfg ${workspaceFolder}\\out\\${workspaceFolderBasename}.ll --disable-output --dot-cfg-dir=${workspaceFolder}\\out",
			"group": "none",
			"problemMatcher": [],
			"options": {
				"cwd": "${workspaceFolder}\\out\\cfg"
			},
			"hide": true
		},
		{
			"label": "clean CFGs",
			"type": "shell",
			"command": "md",
			"args": [
				"${workspaceFolder}\\out\\cfg\\",
				";del",
				"${workspaceFolder}\\out\\cfg\\*.dot*"
			],
			"group": "none",
			"problemMatcher": [],
			"hide": true
		},
		{
			"label": "create CFGs",
			"type": "shell",
			"command": "forfiles /P ${workspaceFolder}\\out\\cfg /C 'cmd /c dot -Tpng -O @file'",
			"problemMatcher": [],
			"dependsOn": [
				"clean CFGs",
				"create cfg-dot files",
			],
			"dependsOrder": "sequence"
		},
		{
			"label": "build print.ll",
			"type": "shell",
			"command": "clang ${workspaceFolder}\\clang\\print.ll -o ${workspaceFolder}\\clang\\print.exe;${workspaceFolder}\\clang\\print.exe",
			"group": "build"
		}
	]
}